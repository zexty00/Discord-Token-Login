{
  "manifest_version": 3,
  "name": "Discord Token Login",
  "version": "1.0",
  "description": "Zaloguj się do Discorda za pomocą tokena.",
  "permissions": ["storage"],
  "host_permissions": ["https://discord.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
